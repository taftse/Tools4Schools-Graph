{
  "name": "tools4schools/graph",
  "description": "The Tools4Schools Graph Api package.",
  "license": "MIT",
  "authors": [
    {
      "name": "Timothy Seebus",
      "email": "timothyseebus@tools4schools.org"
    }
  ],
  "require": {
    "php": "^7.2",
    "illuminate/support": "^6.0",
    "webonyx/graphql-php": "dev-master",
    "symfony/finder": "^4.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0"
  },
  "autoload": {
    "psr-4": {
      "Tools4Schools\\Graph\\": "src/"
    }
  },

  "autoload-dev": {
    "psr-4": {
      "Tools4Schools\\Tests\\Graph\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Tools4Schools\\Graph\\GraphServiceProvider"
      ],
      "aliases": {
        "GraphServer": "Tools4Schools\\Graph\\Support\\Facades"
      }
    }
  },
  "suggest": {
    "illuminate/console": "Required to use the make commands (5.6.*)."
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev"
}
